{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "semicolon": [true, "never"],
    "interface-name": [false],
    "no-console": [false],
    "quotemark": [true, "single"]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["node_modules/**/*.{ts,tsx}", "**/*.{js,jsx,mjs,json}", "src/**/*.spec.*"]
  }
}
